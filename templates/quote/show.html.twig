{% extends 'base.html.twig' %}

{% block title %}Quote{% endblock %}

{% block body %}
    <h1>Quote</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ quote.id }}</td>
            </tr>
            <tr>
                <th>Emited_at</th>
                <td>{{ quote.emitedAt ? quote.emitedAt|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Expired_at</th>
                <td>{{ quote.expiredAt ? quote.expiredAt|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Has_been_signed</th>
                <td>{{ quote.hasBeenSigned ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_quote_index') }}">back to list</a>

    <a href="{{ path('app_quote_edit', {'id': quote.id}) }}">edit</a>

    {{ include('quote/_delete_form.html.twig') }}
{% endblock %}
